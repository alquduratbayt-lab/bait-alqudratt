{
  "expo": {
    "name": "بيت القدرات",
    "slug": "bait-alqudrat",
    "version": "1.0.14",
    "orientation": "portrait",
    "icon": "./assets/app-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/app-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "jsEngine": "hermes",
    "experiments": {
      "turboModules": true
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.baitalqudrat.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.baitalqudrat.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "NOTIFICATIONS"
      ],
      "blockedPermissions": [
        "READ_MEDIA_IMAGES",
        "READ_MEDIA_VIDEO",
        "READ_MEDIA_AUDIO",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "plugins": [
      "expo-notifications",
      "expo-video",
      [
        "expo-image-picker",
        {
          "photosPermission": "بيت القدرات يحتاج الوصول لمكتبة الصور لتحديث صورة ملفك الشخصي. Bait Al-Qudrat needs access to your photo library to update your profile picture.",
          "cameraPermission": "بيت القدرات يحتاج الوصول للكاميرا لالتقاط صورة ملفك الشخصي. Bait Al-Qudrat needs access to your camera to take a profile picture."
        }
      ]
    ],
    "extra": {
      "moyasarPublishableKey": "pk_live_RTLhrirJgwnwTySd34jrqtdmFvWQYKbJZrNGVtpp",
      "eas": {
        "projectId": "0d374624-39fd-4970-8d41-07ce1a3538a3"
      }
    },
    "owner": "alquduratbayt"
  }
}
